<?xml version="1.0" encoding="utf-8" ?>
<NEKTAR>
    <EXPANSIONS>
        <E COMPOSITE="C[6]" NUMMODES="5, 5" BASISTYPE="Modified_A,Modified_B" POINTSTYPE="GaussLobattoLegendre,GaussRadauMAlpha1Beta0" NUMPOINTS="8,7" FIELDS="phi0,phi1,phi2" />
    </EXPANSIONS>
    <CONDITIONS>
        <SOLVERINFO>
            <I PROPERTY="EQTYPE"                        VALUE="Laplace"     />
            <I PROPERTY="Projection"                    VALUE="Continuous"                 />
            <I PROPERTY="GlobalSysSoln"                 VALUE="XxtMultiLevelStaticCond"  />
        </SOLVERINFO>
        
        <VARIABLES>
            <V ID="0"> phi0 </V>
            <V ID="1"> phi1 </V>
            <V ID="2"> phi2 </V>
        </VARIABLES>

        <BOUNDARYREGIONS>
            <B ID="0"> C[1,2,4] </B>    <!-- Farfield -->
            <B ID="1"> C[3] </B>         <!-- Wall -->
            <B ID="2"> C[5] </B>      <!-- Body-->
        </BOUNDARYREGIONS>

        <BOUNDARYCONDITIONS>
            <REGION REF="0">
                <D VAR="phi0" VALUE="0." />
                <D VAR="phi1" VALUE="0." />
                <D VAR="phi2" VALUE="0." />
            </REGION>
            <REGION REF="1">
                <N VAR="phi0" VALUE="0." />
                <N VAR="phi1" VALUE="0." />
                <N VAR="phi2" VALUE="0." />
            </REGION>
            <REGION REF="2">
                <N VAR="phi0" USERDEFINEDTYPE="MOVE" VALUE="0" />
                <N VAR="phi1" USERDEFINEDTYPE="MOVE" VALUE="0" />
                <N VAR="phi2" USERDEFINEDTYPE="MOVE" VALUE="0" />
            </REGION>
        </BOUNDARYCONDITIONS>
        
        <FUNCTION NAME="InitialConditions">
            <E VAR="phi0" VALUE="0" />
            <E VAR="phi1" VALUE="0" />
            <E VAR="phi2" VALUE="0" />
        </FUNCTION>
        
        
        <FUNCTION NAME="ExactSolution">
            <E VAR="phi0" VALUE="0" />
            <E VAR="phi1" VALUE="0" />
            <E VAR="phi2" VALUE="0" />
        </FUNCTION>
    </CONDITIONS>
</NEKTAR>
